  <ps_section howmany="1">
    <ps name="CLASTB-V.P.Z-_" mylink="commonps" enclabels="" sections="1" secttype="Operation">
      <pstext mayhavelinks="1" section="Execute" rep_section="execute">CheckSVEEnabled();
integer elements = VL DIV esize;
bits(PL) mask = P[g];
bits(esize) operand1 = <a link="impl-aarch64.V.read.1" file="shared_pseudocode.xml" hover="accessor: bits(width) V[integer n]">V</a>[dn];
bits(VL) operand2 = Z[m];
bits(esize) result;
integer last = LastActiveElement(mask, esize);

if last &lt; 0 then
    result = <a link="impl-shared.ZeroExtend.1" file="shared_pseudocode.xml" hover="function: bits(N) ZeroExtend(bits(M) x)">ZeroExtend</a>(operand1);
else
    if !isBefore then
        last = last + 1;
        if last &gt;= elements then last = 0;
    result = <a link="impl-shared.Elem.read.3" file="shared_pseudocode.xml" hover="accessor: bits(size) Elem[bits(N) vector, integer e, integer size]">Elem</a>[operand2, last, esize];

<a link="impl-aarch64.V.write.1" file="shared_pseudocode.xml" hover="accessor: V[integer n] = bits(width) value">V</a>[dn] = result;</pstext>
    </ps>
