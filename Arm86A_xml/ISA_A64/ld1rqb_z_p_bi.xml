        <asmtemplate><text>LD1RQB  </text><text>{</text><text> </text><a link="zt" hover="Scalable vector register to be transferred (field &quot;Zt&quot;)">&lt;Zt&gt;</a><text>.B </text><text>}</text><text>, </text><a link="pg" hover="Governing scalable predicate register P0-P7 (field &quot;Pg&quot;)">&lt;Pg&gt;</a><text>/Z, [</text><a link="xn_sp" hover="64-bit general-purpose base register or SP (field &quot;Rn&quot;)">&lt;Xn|SP&gt;</a><text>{</text><text>, #</text><a link="imm" hover="Optional signed immediate byte offset, multiple of 16 [-128-112], default 0 (field &quot;imm4&quot;)">&lt;imm&gt;</a><text>}</text><text>]</text></asmtemplate>
