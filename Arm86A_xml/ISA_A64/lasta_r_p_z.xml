<instructionsection id="lasta_r_p_z" title="LASTA (scalar)" type="instruction">
  <docvars>
    <docvar key="instr-class" value="sve" />
    <docvar key="isa" value="A64" />
    <docvar key="mnemonic" value="LASTA" />
