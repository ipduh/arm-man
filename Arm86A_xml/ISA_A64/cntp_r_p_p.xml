  <ps_section howmany="1">
    <ps name="CNTP-R.P.P-_" mylink="commonps" enclabels="" sections="1" secttype="Operation">
      <pstext mayhavelinks="1" section="Execute" rep_section="execute">CheckSVEEnabled();
integer elements = VL DIV esize;
bits(PL) mask = P[g];
bits(PL) operand = P[n];
bits(64) sum = <a link="impl-shared.Zeros.0" file="shared_pseudocode.xml" hover="function: bits(N) Zeros()">Zeros</a>();

for e = 0 to elements-1
    if ElemP[mask, e, esize] == '1' &amp;&amp; ElemP[operand, e, esize] == '1' then
        sum = sum + 1;
<a link="impl-aarch64.X.write.1" file="shared_pseudocode.xml" hover="accessor: X[integer n] = bits(width) value">X</a>[d] = sum;</pstext>
    </ps>
  </ps_section>
