<instructionsection id="fmul_z_zz" title="FMUL (vectors, unpredicated)" type="instruction">
  <docvars>
    <docvar key="instr-class" value="sve" />
    <docvar key="isa" value="A64" />
    <docvar key="mnemonic" value="FMUL" />
